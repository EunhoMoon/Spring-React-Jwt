<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="study.janek.springreactjwt.mapper.BoardMapper">
	
	<select id="getBoardList" resultType="study.janek.springreactjwt.model.Board">
		SELECT * FROM board ORDER BY writeDate DESC LIMIT #{sNum}, 10
	</select>
	
	<select id="getListSize" resultType="int">
		SELECT COUNT(*) FROM board 
	</select>

</mapper>